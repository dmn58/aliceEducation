
<div class="row">
  <div class="col m6 offset-m3 center">
    <h2>Log in</h2>
  <%= link_to "Sign in with Google", user_google_oauth2_omniauth_authorize_path , class: "btn red darken-1" %>
    <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
      <% render partial: "shared/devisemes" %>
      <div class="input-field">
        <%= f.label :email %><br />
        <%= f.email_field :email, autofocus: true %>
      </div>

      <div class="input-field">
        <%= f.label :password %><br />
        <%= f.password_field :password, autocomplete: "off" %>
      </div>

    <!--   <% if devise_mapping.rememberable? -%>
        <div class="input-field">
          <%= f.check_box :remember_me %>
          <%= f.label :remember_me %>
        </div>
      <% end -%> -->

      <div class="btn waves-effect waves-light">
        <%= f.submit "Log in" %>
      </div>
      <%= link_to "Forgot password?", new_user_password_path, :class => "btn waves-effect waves-light" %>
    <% end %>

  </div>
</div>

<%= render "devise/shared/links" %>
